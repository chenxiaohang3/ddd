package a;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		try {
			String s = "";
			Scanner sc = new Scanner(System.in);
			s = sc.next();
			int l = s.length();
			if (l > 100) {
				return;
			}
			String indexStr = s.charAt(l - 1) + "";
			String L = "";
			L = sc.next();
			int Llen = L.length();
			if (Llen > 500000) {
				return;
			}
			int my_l_index = 0;
			int my_s_index = 0;
			int if_success = 0;
			for (int j = my_l_index; j < Llen; j++) {
				if (my_s_index == l) {
					break;
				}
				char cs = s.charAt(my_s_index);
				char ls = L.charAt(j);
				System.out.println("第" + my_s_index + "个s的字符是" + cs);
				System.out.println("第" + j + "个l的字符是" + ls);
				System.out.println("l-mysindex=" + (l - my_s_index));
				System.out.println("Llen - j=" + (Llen - j));
				if (cs == ls) {
					my_s_index++;
				}
				if (l - my_s_index > Llen - j) {
					if_success = 1;
					break;
				}
			}
			System.out.println("aaaaaaaaaa=" + if_success);
			if (if_success == 1) {
				System.out.println(-1);
			} else {
				System.out.println(L.lastIndexOf(s.charAt(s.length() - 1)));
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
